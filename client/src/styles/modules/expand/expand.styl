@import "nib"
@import "core/fonts"

.expand
  position relative
  text-align center

.expand-entry
  display inline-block
  height 80px
  border 2px solid #e2e2e2
  box-shadow: inset 0 1px 0px #fff, 0 1px 0 #fff
  border-radius( 2px )
  cursor pointer

.expand-entry__text
  font normal 16px/80px regular
  color #c4c3c3
  padding-left 80px
  padding-right 80px
  text-shadow 0px 2px 0px #fff
